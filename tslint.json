{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "jsx-boolean-value": false,
  "rules": {
    "no-console": [
      false
    ],
    "only-arrow-functions": [
      false
    ]
  },
  "defaultSeverity": "none"
}
